# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'taxiShare' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'DshareFramework', :git => 'https://github.com/valems92/DshareFramework.git', :tag => '1.1.0'

  # Pods for Place autocomplete
  pod 'GooglePlaces'

  # pod for the Chat
  pod 'JSQMessagesViewController'

end
